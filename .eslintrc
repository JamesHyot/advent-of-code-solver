{
  "extends": "@antfu",
  "rules": {
    "semi": "off",
    "@typescript-eslint/semi": ["error", "always"],
    "brace-style": "error",
    "@typescript-eslint/brace-style": ["error", "stroustrup", { "allowSingleLine": false }],
    "vue/brace-style": ["error", "stroustrup", { "allowSingleLine": false }],
    "curly": ["error", "multi-line"]
  }
}